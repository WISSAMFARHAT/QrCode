{
  "builds": [
    {
      "name": "dev",
      "js": {
        "declaration": false,
        "webClean": true
      }
    }
  ],
  "files": [
    {
      "output": "wwwroot/css/index.css",
      "input": "src/css/index.less"
    },
    {
      "output": "wwwroot/css/add.css",
      "input": "src/css/add.less"
    },
    {
      "output": "wwwroot/css/site.css",
      "input": [ "src/css/site.less", "src/css/Common/cloud.less", "src/css/add.less", "src/css/index.less" ]
    }
  ],
  "images": [
    {
      "output": "wwwroot/img",
      "input": "src/img/**"
    }
  ]
}